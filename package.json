	"dependencies": {
		"nuxt": "^3.11.1",
		"vue": "^3.4.21",
		"vue-router": "^4.3.0"
	},
	"devDependencies": {
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-vue": "^9.24.0",
		"jsdoc": "^4.0.2",
		"mongoose": "^8.2.3",
		"prettier": "^3.2.5",
		"typedoc": "^0.25.12",
		"typescript": "^5.4.3",
		"vitest": "^1.4.0",
		"vue-tsc": "^1.8.27"
	}
